						floatField -edit -v ($BearID / 25.4)  BFloatField_21;
						floatField -edit -v $OD  BFloatField_22;
						floatField -edit -v ($OD / 25.4)  BFloatField_23;
						
						floatField -edit -v $W  BFloatField_24;
						floatField -edit -v ($W / 25.4)  BFloatField_25;
						
						floatField -edit -v $BD  BFloatField_26;
						floatField -edit -v ($BD / 25.4)  BFloatField_27;
						
						intField -edit -v $N BIntField_2;
					}
					
				 if ($BrR == 2)
					{
						$i = ($BrT -2);
						$OD = ($BrROD2[$i] * 1.0);
						$W = ($BrRWidth2[$i] * 1.0);
						$BD =  ($BallD2[$i] * 1.0);
						$N = $NoBalls2[$i];
	//					print $OD; print "\n";
						
						floatField -edit -v ($BearID / 25.4)  BFloatField_21;
						floatField -edit -v $OD  BFloatField_22;
						floatField -edit -v ($OD / 25.4)  BFloatField_23;
						
						floatField -edit -v $W  BFloatField_24;
						floatField -edit -v ($W / 25.4)  BFloatField_25;
						
						floatField -edit -v $BD  BFloatField_26;
						floatField -edit -v ($BD / 25.4)  BFloatField_27;
						
						intField -edit -v $N BIntField_2;
					}
					
				 if ($BrR == 3)
					{
						$i = ($BrT -2);
						$OD = ($BrROD3[$i] * 1.0);
						$W = ($BrRWidth3[$i] * 1.0);
						$BD =  ($BallD3[$i] * 1.0);
						$N = $NoBalls3[$i];
	//					print $OD; print "\n";
						
						floatField -edit -v ($BearID / 25.4)  BFloatField_21;
						floatField -edit -v $OD  BFloatField_22;
						floatField -edit -v ($OD / 25.4)  BFloatField_23;
						
						floatField -edit -v $W  BFloatField_24;
						floatField -edit -v ($W / 25.4)  BFloatField_25;
						
						floatField -edit -v $BD  BFloatField_26;
						floatField -edit -v ($BD / 25.4)  BFloatField_27;
						
						intField -edit -v $N BIntField_2;
					}
					
				 if ($BrR == 4)
					{
						$i = ($BrT -2);
						$OD = ($BrROD4[$i] * 1.0);
						$W = ($BrRWidth4[$i] * 1.0);
						$BD =  ($BallD4[$i] * 1.0);
						$N = $NoBalls4[$i];
	//					print $OD; print "\n";
						
						floatField -edit -v ($BearID / 25.4)  BFloatField_21;
						floatField -edit -v $OD  BFloatField_22;
						floatField -edit -v ($OD / 25.4)  BFloatField_23;
						
						floatField -edit -v $W  BFloatField_24;
						floatField -edit -v ($W / 25.4)  BFloatField_25;
						
						floatField -edit -v $BD  BFloatField_26;
						floatField -edit -v ($BD / 25.4)  BFloatField_27;
						
						intField -edit -v $N BIntField_2;
					}
					
				 if (($BrN == 3) && ($BrR > 0))
					{
						$i = ($BrT -2);
						$OD = ($BrNOD[$i] * 1.0);
						$W = ($BrNWidth[$i] * 1.0);
						$BD = $NeedleD[$i];
						$N = $NoBalls1[$i];
						$N = (22.0/7.0) * (($OD + $BearID) * 0.5) / ($BD * 1.1);
						
						print $OD; print "\n";
						
						print $W; print "\n";
						print $BD; print "\n";
						
						floatField -edit -v ($BearID / 25.4)  BFloatField_21;
						floatField -edit -v $OD  BFloatField_22;
						floatField -edit -v ($OD / 25.4)  BFloatField_23;
						
						floatField -edit -v $W  BFloatField_24;
						floatField -edit -v ($W / 25.4)  BFloatField_25;
						
						floatField -edit -v $BD  BFloatField_26;
						floatField -edit -v ($BD / 25.4)  BFloatField_27;
						
						intField -edit -v $N BIntField_2;
