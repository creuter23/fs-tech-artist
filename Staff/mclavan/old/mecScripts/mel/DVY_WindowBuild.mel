				move -r (($FrWide[3] + $FH) * -0.5) $FH 0 $Temps[0];
				parent $Temps[0] $WindowGrp[0];
				
			$Temps =  DVY_WindowMaker($T, 3, 0, $E, 3, $FrHigh[1], ($FrWide[3] * 1.01));
				move -r (($FrWide[3] + $FH) * 0.5) $FH 0 $Temps[0];
				parent $Temps[0] $WindowGrp[0];
				
			$Temps =  DVY_WindowMaker($T, 2, 1, $E, 4, $FrHigh[0], ($FrWide[3] * 1.01));
				move -r ($FrWide[3] * 1.5 + $FH * 1.5) $FH 0 $Temps[0];
				parent $Temps[0] $WindowGrp[0];
				
			$Temps =  DVY_WindowMaker($T, 0, 1, $E, 5, $FrHigh[3], $FrWide[3]);
				move -r (($FrWide[3] + $FH) * -0.5) ($Height * 0.75) 0 $Temps[0];
				parent $Temps[0] $WindowGrp[0];
				
			$Temps =  DVY_WindowMaker($T, 0, 1, $E, 6, $FrHigh[3], $FrWide[3]);
				move -r (($FrWide[3] + $FH) * 0.5) ($Height * 0.75) 0 $Temps[0];
				parent $Temps[0] $WindowGrp[0];
		}
		
// -------------------------------------------------------------------------------- //
	
// ******************************************************************************** //	
// Scale finished unit.
	
	scale $DVYUnit $DVYUnit $DVYUnit $WindowGrp[0];
		select -r $WindowGrp[0];
		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;
		
// -------------------------------------------------------------------------------- //
	
// ******************************************************************************** //	
// Lock off attributes.
		
	string $AttrA[] = {".tx", ".ty", ".tz", ".rx", ".ry", ".rz", ".sx", ".sy", ".sz"};
	string $AttrB[] = {".rx", ".ry", ".rz", ".sx", ".sy", ".sz"};
	string $AttrR[] = {".rx", ".ry", ".rz"}; 
	string $AttrS[] = {".sx", ".sy", ".sz"};
	string $AttrT[] = {".tx", ".ty", ".tz"};
	
	select -r -hi $WindowGrp[0];
	string $DSel[] = `ls -tr -sl`;
	
	for ($i= 1; $i < size($DSel); $i++)
		{
			for ($Att in $AttrA)
				{ setAttr -l 1 -k 0 ($DSel[$i] + $Att); }
		}
		
	select -cl;
		catch (`select -r ("*_LeftHung_*_GrpN_*")`);
		catch (`select -add ("*_RightHung_*_GrpN_*")`);
		$DSel = `ls -tr -sl`;
		
		if (size ($DSel) > 0)
			{
				for ($i= 0; $i < size($DSel); $i++)
					{
						setAttr -l 0 -k 1 ($DSel[$i] + ".ry");
					}
			}
		
	select -cl;
		catch (`select -r ("*_TopHung_*_GrpN_*")`);
		catch (`select -add ("*_BotHung_*_GrpN_*")`);
		$DSel = `ls -tr -sl`;
		
		if (size ($DSel) > 0)
			{
			for ($i= 0; $i < size($DSel); $i++)
				{
					setAttr -l 0 -k 1 ($DSel[$i] + ".rx");
				}
			}
			
		for ($Att in $AttrS)
			{ setAttr -l 1 -k 0 ($WindowGrp[0] + $Att); }
			
		
	print "Completed building Window Davvey  2006..\n";
		
}