global proc mecGuiBFormLayoutDis( int $mecNum)
{
      //Enable Control
       intFieldGrp -e -enable true mecGuiBFormLayoutObj10;

      //Enable Offset
       intField -e -enable true mecGuiBFormLayoutObj11;

      //Enable Postion
       textFieldGrp -e -enable true mecGuiBFormLayoutObj12;


      
      if( $mecNum == 1)
      {
          intField -e -enable false mecGuiBFormLayoutObj11;
          textFieldGrp -e -enable false mecGuiBFormLayoutObj12;
      }
      
      if( $mecNum == 2 )
      {
          intField -e -enable false mecGuiBFormLayoutObj11;
          textFieldGrp -e -enable false mecGuiBFormLayoutObj12;
      }
      
      if( $mecNum == 3 )
      {
          intField -e -enable false mecGuiBFormLayoutObj11;
      }
      
      if( $mecNum == 4 )
      {
          intField -e -enable false mecGuiBFormLayoutObj11;
      }
      
      if( $mecNum == 5 )
      {
          textFieldGrp -e -enable false mecGuiBFormLayoutObj12;
      }

      if( $mecNum == 6 )
      {
          intFieldGrp -e -enable false mecGuiBFormLayoutObj10;
          intField -e -enable false mecGuiBFormLayoutObj11;
          textFieldGrp -e -enable false mecGuiBFormLayoutObj12;
      }
}

global proc mecGuiBFormLayoutWork()
{
       int $mecWidth = `intField -q -v mecGuiBFormLayoutObj1`;
       int $mecHeight = `intField -q -v mecGuiBFormLayoutObj2`;
       string $mecParent = `textField -q -tx mecGuiBFormLayoutObj3`;
       string $mecAnn = `textField -q -tx mecGuiBFormLayoutObj4`;
       int $mecDiv = `intField -q -v mecGuiBFormLayoutObj5`;
       string $mecObjName = `textFieldGrp -q -tx mecGuiBFormLayoutObj6`;


       string $mecFinalStr = "formLayout ";
       
       //Width
       if(`checkBox -q -v mecGuiBFormLayoutCk1`)
            $mecFinalStr += "-w " + $mecWidth + " ";

       //Height
       if(`checkBox -q -v mecGuiBFormLayoutCk2`)
            $mecFinalStr += "-h " + $mecHeight + " ";

       //Parent
       if(`checkBox -q -v mecGuiBFormLayoutCk3`)
            $mecFinalStr += "-p \"" + $mecParent + "\" ";

       //Annotation
       if(`checkBox -q -v mecGuiBFormLayoutCk4`)
            $mecFinalStr += "\n-ann \"" + $mecAnn + "\"\n";


       //Divisions
       $mecFinalStr += "-nd " + $mecDiv + " ";

       $mecFinalStr += $mecObjName + ";\n";
       
       scrollField -e -ip 0 -insertText $mecFinalStr mecGuiBSf1;

}

//This is the second part of the form layout adding elements to the form.
global proc mecGuiBFormLayoutWork2()
{
       string $mecAttach = `radioCollection -q -sl mecGuiBFormLayoutRCollection`;
       string $mecControl = `textFieldGrp -q -tx mecGuiBFormLayoutObj8`;
       string $mecEdge = `optionMenu -q -v mecGuiBFormLayoutObj9`;
       int $mecOffset = `intFieldGrp -q -v1 mecGuiBFormLayoutObj10`;
       int $mecPosition = `intField -q -v mecGuiBFormLayoutObj11`;
       string $mecAttControl = `textFieldGrp -q -tx mecGuiBFormLayoutObj12`;
       string $mecMainObj = `textFieldGrp -q -tx mecGuiBFormLayoutObj13`;
       
       string $mecFinalStr = "formLayout -e ";

       //if Attach form
       if($mecAttach == "mecGuiBFormLayoutRB1")
       {
           $mecFinalStr += "-af " + $mecControl + " \"" + $mecEdge + "\" " + $mecOffset + " ";
       }

       //if attach opposite form
       if($mecAttach == "mecGuiBFormLayoutRB2")
       {
           $mecFinalStr += "-aof " + $mecControl + " \"" + $mecEdge + "\" " + $mecOffset + " ";
       }

       //if attach control
       if($mecAttach == "mecGuiBFormLayoutRB3")
       {
           $mecFinalStr += "-ac " + $mecControl + " \"" + $mecEdge + "\" " + $mecOffset + " " + $mecAttControl + " ";
       }

       //if attach opposite form
       if($mecAttach == "mecGuiBFormLayoutRB4")
       {
           $mecFinalStr += "-aoc "  + $mecControl + " \"" + $mecEdge + "\" " + $mecOffset + " " + $mecAttControl + " ";
       }

       //if attach position
       if($mecAttach == "mecGuiBFormLayoutRB5")
       {
           $mecFinalStr += "-ap " + $mecControl + " \"" + $mecEdge + "\" " + $mecOffset + " " + $mecPosition + " ";
       }

       //if attach None
       if($mecAttach == "mecGuiBFormLayoutRB6")
       {
           $mecFinalStr += "-an " + $mecControl + " \"" + $mecEdge + "\" ";
       }

       $mecFinalStr += $mecMainObj + ";\n";
       
       scrollField -e -ip 0 -insertText $mecFinalStr mecGuiBSf1;

}
