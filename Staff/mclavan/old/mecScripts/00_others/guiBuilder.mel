// work for the column layout frame
global proc mecGuiBLayoutsColWork()
{
	// After hitting apply button on the ColumnLayout button.
	//   Info will be grabed stored into a string and then sent to the scrollField.
	string $mecFinalStr = "columnLayout ";
	
	string $mecObjName = `textField -q -text mecGuiBLayoutsColTf1`;
	int $mecWidth = `intField -q -v mecGuiBLayoutsColTf2`;
	int $mecHeight = `intField -q -v mecGuiBLayoutsColTf3`;
	string $mecParent = `textField -q -text mecGuiBLayoutsColTf4`;

	//checking to see if the checkbox is checked.

	if(`checkBox -q -v mecGuiBLayoutsColCk2 `)
	{
		$mecFinalStr += "-w " + $mecWidth + " ";
	}

	if(`checkBox -q -v mecGuiBLayoutsColCk3`)
	{
		$mecFinalStr += "-h " + $mecHeight + " ";
	}

	if(`checkBox -q -v mecGuiBLayoutsColCk4`)
	{
		$mecFinalStr += "-p " + $mecParent + " ";
	}

	if(`checkBox -q -v mecGuiBLayoutsColCk1`)
	{
		$mecFinalStr += $mecObjName;
	}
	
	$mecFinalStr += ";\n";
	// inserting text into the scrollList
	scrollField -e -ip 0 -insertText $mecFinalStr mecGuiBSf1;
/**/

}


//================= Row Layout =======================================================
global proc mecGuiBLayoutsRowGUI()
{
	columnLayout -p mecGuiBfRowL -w 300 mecGuiBLayoutsRowCl;
	
	rowLayout -nc 2 -columnWidth2 100 200;
	checkBox -l "Object Name" -w 100
		-onc "textField -e -enable true mecGuiBLayoutsRowTf1"
		-ofc "textField -e -enable false mecGuiBLayoutsRowTf1"
		mecGuiBLayoutsRowCk1;
	textField -w 175 -enable false mecGuiBLayoutsRowTf1;
	setParent mecGuiBLayoutsRowCl;
	
	rowLayout -nc 4 -columnWidth4 75 75 75 75;
	checkBox -l "Width" -w 75
		-onc "textField -e -enable true mecGuiBLayoutsRowTf2"
		-ofc "textField -e -enable false mecGuiBLayoutsRowTf2"
		mecGuiBLayoutsRowCk2;
	intField -w 50 -enable false mecGuiBLayoutsRowTf2;
	
	checkBox -l "Height" -w 75
		-onc "textField -e -enable true mecGuiBLayoutsRowTf3"
		-ofc "textField -e -enable false mecGuiBLayoutsRowTf3"
		mecGuiBLayoutsRowCk3;
	intField -w 50 -enable false mecGuiBLayoutsRowTf3;
	setParent mecGuiBLayoutsRowCl;

	rowLayout -nc 2 -columnWidth2 75 200;
	checkBox -l "Parent" -w 75
		-onc "textField -e -enable true mecGuiBLayoutsRowTf4"
		-ofc "textField -e -enable false mecGuiBLayoutsRowTf4"
		mecGuiBLayoutsRowCk4;
	textField -w 200 -enable false mecGuiBLayoutsRowTf4;
	setParent mecGuiBLayoutsRowCl;

	// This is textField will cause change in the procedures underneight
	//   the columnWidth and so on must change according to how many row there are.
	rowLayout -nc 2 -columnWidth2 110 40;
	text -l "Number of Columns";
	// Will rerun this procedure when this field is changed.
	intField -w 40 -min 2 -v 2 -cc "mecguiBLayoutsRowDynamic(`setParent -q` )" mecGuiBLayoutsRowTf5;
	setParent mecGuiBLayoutsRowCl;
	

	// This column with be what the columnWidth will be fixed to.
	columnLayout mecGuiBLayoutsRowDyColTop;
	// This columnLayout will be constantly genterated and deleted.
	columnLayout mecGuiBLayoutsRowDyCol;
	// This is generate the columnWidth fields
	mecguiBLayoutsRowDynamic("mecGuiBLayoutsRowCl");

	setParent mecGuiBLayoutsRowCl;
	
        
        text -l "";
        rowLayout -nc 2 -cw2 120 100;
	button -l "Add Row Layout" -c "mecGuiBLayoutsRowWork";
	        
        button -l "Help"
	       -ann "This won't work on maya versions before 6.0."
               -c "mecGuiBHelpWin( \"http://localhost:4446/Maya6.0/en_US/Commands/rowLayout.html\", \"Row Layout\")";
	setParent mecGuiBLayoutsRowCl;
        
        text -l "";
}

// Generation of the columnWidth fields.
global proc mecguiBLayoutsRowDynamic(string $mecParent)
{
	// Must set the parent so that the layout is going in the right spot.	
	setParent mecGuiBLayoutsRowDyColTop;
	deleteUI mecGuiBLayoutsRowDyCol;
	columnLayout mecGuiBLayoutsRowDyCol;
	
	//Dynamicly Generated controls.
	//ColumnWidth flag
	$mecNC = `intField -q -v mecGuiBLayoutsRowTf5`;
	if( $mecNC > 0 )
	{	
		int $mecI = 1;
		for( $mecI; $mecI <= $mecNC; $mecI++  )
		{
			string $mecName = "columnWidth" + $mecI;
			rowLayout -nc 3 -columnWidth3 100 60 135;
			text -l $mecName;
			string $mecObj = "mecGuiBLayoutsRowCW" + $mecI;
			intField -w 40 $mecObj;
			text -l "" -w 135;
			setParent..;
		}
	}
	
	setParent $mecParent;

}

// This procedure will put in the text into the scrollField for the rowLayout.
global proc mecGuiBLayoutsRowWork()
{
	// Storing all the variables.
	string $mecFinalStr;
	
	string $mecObjName = `textField -q -text mecGuiBLayoutsRowTf1`;
	int $mecWidth = `intField -q -v mecGuiBLayoutsRowTf2`;
	int $mecHeight = `intField -q -v mecGuiBLayoutsRowTf3`;
	string $mecParent = `textField -q -text mecGuiBLayoutsRowTf4`;
	int $mecNC = `intField -q -v mecGuiBLayoutsRowTf5`;

	$mecFinalStr += "rowLayout ";
	
	//parent flag
	if(`checkBox -q -v mecGuiBLayoutsRowCk4 `)
	{
		$mecFinalStr += "-p " + $mecParent + " ";
	}
	
	// width flag
	if(`checkBox -q -v mecGuiBLayoutsRowCk2 `) 
	{
		$mecFinalStr += "-w " + $mecWidth + " ";
	}

	// height flag
	if(`checkBox -q -v mecGuiBLayoutsRowCk3 `)
	{
		$mecFinalStr += "-h " + $mecHeight + " ";
	}

	// number of columns flag
	$mecFinalStr += "-nc " + $mecNC + " ";

	//int $mecCW[];
	string $mecCW = "-columnWidth" + $mecNC + " ";
	int $mecI = 1;
	
	//columnWidth flag
	$mecFinalStr += $mecCW; 
	for( $mecI ; $mecI <= `intField -q -v mecGuiBLayoutsRowTf5` ; $mecI ++)
	{	
		string $mecObj = "mecGuiBLayoutsRowCW" + $mecI;
		$mecFinalStr += `intField -q -v $mecObj` + " ";
	}

	//objectName
	if(`checkBox -q -v mecGuiBLayoutsRowCk1 `)
	{
		$mecFinalStr += $mecObjName;

	}

		

	$mecFinalStr += ";\n";
	// inserting text into the scrollField
	scrollField -e -ip 0 -insertText $mecFinalStr mecGuiBSf1;
}

//================= Frame Layout =====================================================

global proc mecGuiBLayoutsFrameGUI()
{
