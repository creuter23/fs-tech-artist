        string $mecLabel = `textFieldGrp -q -text mecGuiBLayoutsFrameTf1`;
        string $mecBorderA = `optionMenuGrp -q -v mecGuiBLayoutsFrameTf2`;
        string $mecBorderS = `optionMenuGrp -q -v mecGuiBLayoutsFrameTf3`;
        int $mecCl = `checkBox -q -v mecGuiBLayoutsFrameCk5`;  //Chk
        int $mecCll = `checkBox -q -v mecGuiBLayoutsFrameCk4`; //Chk

