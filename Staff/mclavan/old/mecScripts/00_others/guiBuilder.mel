        if(`checkBox -q -v mecGuiBMiscPictureCk2`)
                     $mecFinalStr += "-h " + $mecHeight + " ";

        if(`checkBox -q -v mecGuiBMiscPictureCk3`)
                     $mecFinalStr += "-p " + $mecParent + " ";

        $mecFinalStr += "-i \"" + $mecImage + "\";\n";


        scrollField -e -ip 0 -insertText $mecFinalStr mecGuiBSf1;

}

