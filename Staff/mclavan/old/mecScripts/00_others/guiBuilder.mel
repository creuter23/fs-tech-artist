        if(`checkBox -q -v mecGuiBMiscImageCk1`)
                     $mecFinalStr += "-w " + $mecWidth + " ";

        if(`checkBox -q -v mecGuiBMiscImageCk2`)
                     $mecFinalStr += "-h " + $mecHeight + " ";

        if(`checkBox -q -v mecGuiBMiscImageCk3`)
                     $mecFinalStr += "-p " + $mecParent + " ";

        $mecFinalStr += "-i \"" + $mecImage + "\";\n";


        scrollField -e -ip 0 -insertText $mecFinalStr mecGuiBSf1;

}

