		-onc "textField -e -enable true mecGuiBLayoutsColTf2"
		-ofc "textField -e -enable false mecGuiBLayoutsColTf2"
		mecGuiBLayoutsColCk2;
	intField -w 50 -enable false mecGuiBLayoutsColTf2;
	
	checkBox -l "Height" -w 75
		-onc "textField -e -enable true mecGuiBLayoutsColTf3"
		-ofc "textField -e -enable false mecGuiBLayoutsColTf3"
		mecGuiBLayoutsColCk3;
	intField -w 50 -enable false mecGuiBLayoutsColTf3;
	setParent..;

	rowLayout -nc 2 -columnWidth2 75 200;
	checkBox -l "Parent" -w 75
