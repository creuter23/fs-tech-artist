// Window Pref
//   This allows you to actively change what the window will look like.  This option
//      area is different from the other because this menu is accessed every time a
//      window is regenerated.  Unlike the other menu which just generate some code.
global proc mecGuiBWindowPref( string $mecParent)
{
	
	
	columnLayout ;
	text -l "" -h 10;
	textFieldGrp -l "Title" -h 25 -text "Generated Window" -cw2 75 225 mecGuiBtfgTitle;

	rowLayout -numberOfColumns 2 -columnWidth2 150 150;

	intFieldGrp -l "Width" -h 25 -cw2 75 40 -v1 300 mecGuiBifgWidth;
	intFieldGrp -l "Height" -cw2 75 40 -v1 300 mecGuiBifgHeight;

	setParent..;
        rowLayout -w 150 -nc 2 -columnWidth2 75 75 ;
        separator -w 75 -h 25 -hr 1 -st "none";

        	checkBox -l "Absolute" -v 1 -w 75 
                         -ann "w/h value will reflect the values above else resizeing the window will change w/h above." mecGuiBifgRelative;
        
        setParent..;
        	textFieldGrp -l "ObjName" -cw2 75 225 -text "mecGuiBGeneratedWin" mecGuiBWinObj;
                
        	
                //intFieldGrp -l "Sizeable" -cw2 75 40 -v1 1 mecGuiBifgSizeable;
        text -l "" -w 300 -h 15;
        rowLayout -w 300 -nc 4 -columnWidth4 75 75 25 75 ;
        separator -w 25 -hr 1 -st "none";
        checkBox -w 75 -l "Sizeable" -v 1
        		-align "center" mecGuiBifgSizeable;
        separator -w 25 -hr 1 -st "none";
        
        checkBox -w 75 -l "Toolbox"
        		-align "center" mecGuiBifgToolB;

        
        setParent..;
        rowLayout -w 300 -nc 4 -columnWidth4 75 75 25 75 ;
        separator -w 25 -hr 1 -st "none";
        checkBox -w 75 -l "Minimize" -v 1
        		-align "center" mecGuiBifgMin;

        separator -w 25 -hr 1 -st "none";
        
        checkBox -w 75 -l "Maximize" -v 1
        		-align "center" mecGuiBifgMax;
        
        setParent..;
        rowLayout -w 300 -nc 4 -columnWidth4 75 75 25 75;
        separator -w 25 -hr 1 -st "none";
        checkBox -w 75 -l "Menu Bar"  -v 1
        		-align "center" mecGuiBifgMenuB;
        
        separator -w 25 -hr 1 -st "none";
        checkBox -w 75 -l "Title Bar" -v 1
        		-align "center" mecGuiBifgTitleB;
        setParent $mecParent;
} 


//---------------- Layouts --------------------------------------------------------
// Each of frames is composed of three procedures.
// 1) setup the frame
// 2) setup the gui for that control (procedure called with in the main layout window)
// 3) do the work required to pass the info. (procedure called with in the Gui procedure)
//         
//---------------------------------------------------------------------------------

// Main layout menu where all the frames are stored.
global proc mecGuiBLayouts ( string $mecParent )
{

        paneLayout -w 300 -h 550 mecGuiBcolLayout2;
        scrollLayout -w 300 -h 550 -hst 0 -vst 8;

        columnLayout -w 300 mecGuiBcolLayout;
        



	frameLayout -cll true -cl true -w 300 -l "Pane Layout" -bs "etchedIn" mecGuiBPaneLFrame;
	mecGuiBPaneLayoutGui();
	setParent mecGuiBcolLayout;

        frameLayout -cll true -cl true -w 300 -l "Form Layout" -bs "etchedIn" mecGuiBGfrmFormL;
        mecGuiBFormLayoutGui();
        setParent mecGuiBcolLayout;

	//frameLayout for Column Layout
	frameLayout -cll true -cl true -w 300 -l "Column Layout" -bs "etchedIn" mecGuiBfrmColumnL;
	// This is were the proc for columnLayout will go.
	mecGuiBLayoutsColGUI();
	setParent mecGuiBcolLayout;
	
	frameLayout -cll true -cl true -w 300 -l "Row Layout" -bs "etchedIn" mecGuiBfRowL;
	mecGuiBLayoutsRowGUI();
	setParent mecGuiBcolLayout;

	frameLayout -cll true -cl true -w 300 -l "Frame Layout" -bs "etchedIn" mecGuiBfFrameL;
	mecGuiBLayoutsFrameGUI();
	setParent mecGuiBcolLayout;
	
	frameLayout -cll true -cl true -w 300 -l "Grid Layout" -bs "etchedIn" mecGuiBGfrmGrid;
	mecGuiBGridLayoutGui();
        setParent mecGuiBcolLayout;
        
        frameLayout -cll true -cl true -w 300 -l "Scroll Layout" -bs "etchedIn" mecGuiBGScrLay;
        mecGuiBScrollLayoutGui();
        setParent mecGuiBcolLayout;
        
        frameLayout -cll true -cl true -w 300 -l "Tab Layout" -bs "etchedIn" mecGuiBTabL;
        mecGuiBTabLayoutGui();
        setParent mecGuiBcolLayout;
        


	setParent $mecParent;
}

