global proc int mecGuiBPaneLayoutReturn( string $mecConfig )
{
       if( $mecConfig == "single" )
       {
             return 1;
       }
       else if( ($mecConfig == "horizontal2") || ($mecConfig == "vertical2"))
       {
             return 2;
       }
       else if( ($mecConfig == "horizontal3") || ($mecConfig == "vertical3") || ($mecConfig == "top3") || ($mecConfig == "left3") || ($mecConfig == "bottom3") || ($mecConfig == "right3") )
       {
             return 3;
       }
       else
       {
             return 4;
       }
}

global proc mecGuiBPaneLayoutDynamic( int $mecNum )
{
       //deleting old layout
         string $mecP = `setParent -q`;

          setParent mecGuiBPaneLayoutTop2;
         deleteUI mecGuiBPaneLayoutDelete2;
  
         columnLayout mecGuiBPaneLayoutDelete2;


        int $mecTemp = 1;
        for($mecTemp ; $mecTemp <= $mecNum ; $mecTemp++)
        {

              string $mecWidth = "mecGuiBPaneLayoutDyW" + $mecTemp;
              string $mecHeight = "mecGuiBPaneLayoutDyH" + $mecTemp;
              string $mecSetP = "mecGuiBPaneLayoutDySetP" + $mecTemp;
              //string $mecSetpCK = "mecGuiBPanelLayoutBySetCK" + $mecTemp;

              //Creating layout of one control
              text -l "" -h 5;
              text -l ("Pane " + $mecTemp) -w 75;
              rowLayout -w 300 -nc 3 -cw3 25 90 90;
              text -l "Pane " -w 25;
              
              rowLayout -w 180 -nc 4 -cw4 50 40 50 40;
              text -w 50 -l "Width" ;
              intField -w 40 -min 0 -max 100 $mecWidth;

              text -w 50 -l "Height";
              intField -w 40 -min 0 -max 100 $mecHeight;
              
              setParent mecGuiBPaneLayoutDelete2;
              /*
              intFieldGrp -w 90 -l "Width" -nf 1 -v1 0
              		-cw2 50 40 $mecWidth;

              intFieldGrp -w 90 -l "Height" -nf 1 -v1 0
              		-cw2 50 40 $mecHeight;
              setParent mecGuiBPaneLayoutDelete2;

              textFieldGrp -w 300 -l "Set Pane" -tx ""
              		-cw2 75 150 $mecSetP;
              		
                */

         } // end for

        //this will add the active pane.
        $mecTemp = 1;
        
	//loop though kill all of them;
        for($mecTemp ; $mecTemp <= 4 ; $mecTemp++)
        {
             if(`menuItem -q -exists ("mecGuiPaneLayoutMenu" + $mecTemp)`)
             {

                  string $mecObj =  "mecGuiPaneLayoutMenu" + $mecTemp;
                  deleteUI $mecObj;
             } //end if

        }// end for
        
        $mecTemp = 1;
        for($mecTemp ; $mecTemp <= $mecNum; $mecTemp++)
        {
              menuItem -p "mecGuiPaneLayoutActiveP" -l $mecTemp ("mecGuiPaneLayoutMenu" + $mecTemp);
        }

 
        setParent $mecP;

}

global proc mecGuiBPaneLayoutWork()
{
	int $mecWidth = `intFieldGrp -q -v1 mecGuiBPaneLayoutObj1`;
	int $mecHeight = `intFieldGrp -q -v1 mecGuiBPaneLayoutObj2`;
	string $mecParent = `textField -q -tx mecGuiBPaneLayoutObj3`;
	int $mecFrameTh = `intFieldGrp -q -v1 mecGuiBPaneLayoutObj4`;
	int $mecSepTh = `intFieldGrp -q -v1 mecGuiBPaneLayoutObj5`;
	string $mecObjName = `textFieldGrp -q -tx mecGuiBPaneLayoutObj6`;
	string $mecConfig = `optionMenuGrp -q -v mecGuiBPaneLayoutConfig`;
	// converting from a string to an int.
        int $mecActive =  `optionMenu -q -v mecGuiPaneLayoutActiveP`;
	
	string $mecFinalStr = "paneLayout ";
	
	$mecFinalStr += "-w " + $mecWidth + " ";
	    
        $mecFinalStr += "-h " + $mecHeight + " ";
	
        if(`checkBox -q -v mecGuiBPaneLayoutCk10`)
            $mecFinalStr += "-p " + $mecParent + " ";
	
	$mecFinalStr += "-aft " + $mecFrameTh + " -st " + $mecSepTh + "\n";
	
	$mecFinalStr += "-configuration \"" + $mecConfig + "\" ";
	
	$mecFinalStr += "-api " + $mecActive + " ";
	
	//Need to loop, Need to see how many times.
	int $mecNum = mecGuiBPaneLayoutReturn($mecConfig);
	int $mecI = 1;
	
	for( $mecI ; $mecI <= $mecNum; $mecI++)
	{
              string $mecW = "mecGuiBPaneLayoutDyW" + $mecI;
              string $mecH = "mecGuiBPaneLayoutDyH" + $mecI;
              string $mecS = "mecGuiBPaneLayoutDySetP" + $mecI;
              int $mecWidth2 = `intField -q -v $mecW`;
              int $mecHeight2 = `intField -q -v $mecH`;
              //string $mecSetP2 = `textFieldGrp -q -tx $mecS`;

              // Do pane size first then do setPane
              //   Need to see if setPane can be done multiple times
              $mecFinalStr += "\n -paneSize " + $mecI + " " + $mecWidth2 + " " + $mecHeight2 + " ";
              //$mecFinalStr += "-setPane \"" + $mecSetP2 + "\" " + $mecI + "\n";

	}
        
        $mecFinalStr += $mecObjName + ";\n";
        
        scrollField -e -ip 0 -insertText $mecFinalStr mecGuiBSf1;

}

