global proc mecGuiBControlsSymbolCheckBoxWork()
{
      int $mecWidth = `intField -q -v mecGuiBControlsSymbolCheckBoxObj1`;
      int $mecHeight = `intField -q -v mecGuiBControlsSymbolCheckBoxObj2`;
      string $mecParent = `textField -q -tx mecGuiBControlsSymbolCheckBoxObj3`;
      string $mecAnn = `textField -q -tx mecGuiBControlsSymbolCheckBoxObj4`;
      int $mecValue = `checkBox -q -v mecGuiBControlsSymbolCheckBoxCk30`;
      string $mecImage = `textFieldGrp -q -tx mecGuiBControlsSymbolCheckBoxObj5`;
      string $mecOnImage = `textField -q -tx mecGuiBControlsSymbolCheckBoxObj6`;
      string $mecOffImage = `textField -q -tx mecGuiBControlsSymbolCheckBoxObj7`;
      string $mecDOnImage = `textField -q -tx mecGuiBControlsSymbolCheckBoxObj8`;
      string $mecDOffImage = `textField -q -tx mecGuiBControlsSymbolCheckBoxObj9`;
      int $mecCC = `checkBox -q -v mecGuiBControlsSymbolCheckBoxCk9`;
      int $mecONC = `checkBox -q -v mecGuiBControlsSymbolCheckBoxCk10`;
      int $mecOFC = `checkBox -q -v mecGuiBControlsSymbolCheckBoxCk11`;
      string $mecObjName = `textFieldGrp -q -tx mecGuiBControlsSymbolCheckBoxObj10`;
      
      string $mecFinalStr = "symbolCheckBox ";
      
      if(`checkBox -q -v mecGuiBControlsSymbolCheckBoxCk1`)
                   $mecFinalStr += "-w " + $mecWidth + " ";

      if(`checkBox -q -v mecGuiBControlsSymbolCheckBoxCk2`)
                   $mecFinalStr += "-h " + $mecHeight + " ";
                   
      if(`checkBox -q -v mecGuiBControlsSymbolCheckBoxCk3`)
                   $mecFinalStr += "-p " + $mecParent + " ";
                   
      if(`checkBox -q -v mecGuiBControlsSymbolCheckBoxCk4`)
                   $mecFinalStr += "\n\t-ann \"" + $mecAnn + "\" ";
      
      $mecFinalStr += "-v " + $mecValue + " ";
      
      $mecFinalStr += "\n\t-i \"" + $mecImage + "\" ";
      //onImage
      if(`checkBox -q -v mecGuiBControlsSymbolCheckBoxCk5`)
                   $mecFinalStr += "\n\t-oni \"" + $mecOnImage + "\" ";
	   //offImage
       if(`checkBox -q -v mecGuiBControlsSymbolCheckBoxCk6`)
                   $mecFinalStr += "\n\t-ofi \"" + $mecOffImage + "\" ";
       //Dissable on Image            
       if(`checkBox -q -v mecGuiBControlsSymbolCheckBoxCk7`)
                   $mecFinalStr += "\n\t-dni \"" + $mecDOnImage + "\" ";
       //Dissable off Image            
       if(`checkBox -q -v mecGuiBControlsSymbolCheckBoxCk8`)
                   $mecFinalStr += "\n\t-dfi \"" + $mecDOffImage + "\" ";
                   
       if($mecCC)
                   $mecFinalStr += "\n\t-cc \"\" ";
                   
       if($mecONC)
                   $mecFinalStr += "\n\t-onc \"\" ";
                   
       if($mecOFC)
                   $mecFinalStr += "\n\tofc \"\" ";
                   
                   
        $mecFinalStr += $mecObjName + ";\n";
        
        scrollField -e -ip 0 -insertText $mecFinalStr mecGuiBSf1;

}
